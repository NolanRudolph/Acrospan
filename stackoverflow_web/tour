                            <span title="74 bronze badges"><span class="badge3"></span><span class="badgecount">74</span></span>
                        </div>
                    </div>

                    <div class="fr">
                        <div class="rep-block fl">
                            <span class="rep-up">+10</span>
                            question voted up
                        </div>
                        <div class="rep-block fl">
                            <span class="rep-up">+10</span>
                            answer voted up
                        </div>
                        <div class="rep-block fl">
                            <span class="rep-up">+15</span>
                            answer is accepted
                        </div>
