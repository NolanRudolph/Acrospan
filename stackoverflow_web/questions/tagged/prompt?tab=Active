        clc.cmd.push(function () { window.clc_request='ArBlBwgAWdgIAAAAAAAAAAACAAAAAQAAAAAIAAAAfHByb21wdHwAbfscyB1JFuQ7vQ'; clc.load(); });
        clc.dfp = clc.dfp || {};
        clc.dfp.targeting = {Registered:['false'],'so-tag':['prompt'],'tag-reportable':['prompt'],'tag-non-reportable':['prompt']};
        var googletag = googletag || {};
        googletag.cmd = googletag.cmd || [];
        googletag.cmd.push(function () { clc.dfp.load(); });

