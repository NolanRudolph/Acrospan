        clc.dfp = clc.dfp || {};
        clc.dfp.targeting = {Registered:['false'],'so-tag':['snowflake-cloud-data-platform'],'tag-reportable':['snowflake-cloud-data-platform'],'tag-non-reportable':['snowflake-cloud-data-platform']};
